{
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "concurrently": "^8.0.1"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "scripts": {
    "start": "node server/server.js",
    "build": "cd client && npm run build && mv build ../server",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  }
}